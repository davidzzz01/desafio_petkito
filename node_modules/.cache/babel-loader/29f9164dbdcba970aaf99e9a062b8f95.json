cesContent":["<template>\r\n  <AppLayout>\r\n    <div class=\"max-w-5xl mx-auto\">\r\n      <h2 class=\"text-2xl font-bold mb-4 text-blue-200\">Usuários</h2>\r\n      <div class=\"flex items-center space-x-2 mb-2\">\r\n        <input\r\n          v-model=\"search\"\r\n          @input=\"fetchUsers\"\r\n          placeholder=\"Buscar usuário...\"\r\n          class=\"border border-blue-400 bg-gray-200 text-gray-800 px-3 py-2 rounded focus:outline-none focus:ring-2 focus:ring-blue-400\"\r\n        />\r\n        <select\r\n          v-model=\"roleFilter\"\r\n          @change=\"fetchUsers\"\r\n          class=\"border border-blue-400 bg-gray-200 text-gray-800 px-3 py-2 rounded focus:outline-none focus:ring-2 focus:ring-blue-400\"\r\n        >\r\n          <option value=\"\">Todos</option>\r\n          <option value=\"admin\">Admin</option>\r\n          <option value=\"user\">Usuário</option>\r\n        </select>\r\n      </div>\r\n      <div v-if=\"!filteredUsers.length\" class=\"text-center text-blue-300 py-8\">\r\n        Nenhum usuário encontrado.\r\n      </div>\r\n      <table v-else class=\"w-full bg-gray-100 rounded-lg shadow text-gray-800\">\r\n        <thead>\r\n          <tr class=\"bg-gray-200\">\r\n            <th class=\"py-2 px-4 text-left\">Nome</th>\r\n            <th class=\"py-2 px-4 text-left\">Email</th>\r\n            <th class=\"py-2 px-4 text-left\">Tipo</th>\r\n            <th class=\"py-2 px-4 text-left\" v-if=\"isAdmin\">Ações</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"user in filteredUsers\" :key=\"user.id\" class=\"hover:bg-blue-100 transition\">\r\n            <td class=\"py-2 px-4\">{{ user.name }}</td>\r\n            <td class=\"py-2 px-4\">{{ user.email }}</td>\r\n            <td class=\"py-2 px-4\">{{ user.is_admin ? 'Administrador' : 'Usuário' }}</td>\r\n            <td class=\"py-2 px-4\" v-if=\"isAdmin\">\r\n              <button @click=\"openModal(user)\" class=\"text-blue-400 hover:text-blue-600 mr-2\" title=\"Ver detalhes\">\r\n                <i class=\"fas fa-eye\"></i>\r\n              </button>\r\n              <router-link :to=\"`/users/${user.id}/edit`\" class=\"text-green-400 hover:text-green-600 mr-2\" title=\"Editar\">\r\n                <i class=\"fas fa-edit\"></i>\r\n              </router-link>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <Modal :show=\"showModal && isAdmin\" @close=\"showModal = false\">\r\n        <div v-if=\"selectedUser\">\r\n          <h3 class=\"text-xl font-bold text-blue-800 mb-2\">{{ selectedUser.name }}</h3>\r\n          <p class=\"text-blue-600 mb-1\">Email: <span class=\"text-blue-900\">{{ selectedUser.email }}</span></p>\r\n          <p class=\"text-blue-600 mb-1\">Tipo: <span class=\"text-blue-900\">{{ selectedUser.is_admin ? 'Administrador' : 'Usuário' }}</span></p>\r\n          <p class=\"text-blue-600 mb-1\">ID: <span class=\"text-blue-900\">{{ selectedUser.id }}</span></p>\r\n        </div>\r\n      </Modal>\r\n    </div>\r\n  </AppLayout>\r\n</template>\r\n\r\n<script>\r\nimport AppLayout from '../components/AppLayout.vue'\r\nimport Modal from '../components/Modal.vue'\r\nimport api from '../services/api'\r\n\r\nexport default {\r\n  components: { AppLayout, Modal },\r\n  data() {\r\n    return {\r\n      users: [],\r\n      showModal: false,\r\n      selectedUser: null,\r\n      search: '',\r\n      roleFilter: ''\r\n    }\r\n  },\r\n  computed: {\r\n    isAdmin() {\r\n      const user = JSON.parse(localStorage.getItem('user'))\r\n      return user && user.is_admin\r\n    },\r\n    filteredUsers() {\r\n      let users = this.users\r\n      if (this.search) {\r\n        users = users.filter(u => u.name.toLowerCase().includes(this.search.toLowerCase()))\r\n      }\r\n      if (this.roleFilter) {\r\n        users = users.filter(u => u.role === this.roleFilter)\r\n      }\r\n      return users\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchUsers() {\r\n      const { data } = await api.get('/users')\r\n      this.users = data\r\n    },\r\n    openModal(user) {\r\n      if (this.isAdmin) {\r\n        this.selectedUser = user\r\n        this.showModal = true\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchUsers()\r\n  }\r\n}\r\n</script> "],"mappings":";;EAESA,KAAK,EAAC;AAAmB;;EAEvBA,KAAK,EAAC;AAAkC;;;EAiBXA,KAAK,EAAC;;;;EAG1BA,KAAK,EAAC;;;EAEZA,KAAK,EAAC;AAAa;;;EAIjBA,KAAK,EAAC;;;EAKNA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAW;;;EACjBA,KAAK,EAAC;;;;;;;EAaRA,KAAK,EAAC;AAAsC;;EAC7CA,KAAK,EAAC;AAAoB;;EAAcA,KAAK,EAAC;AAAe;;EAC7DA,KAAK,EAAC;AAAoB;;EAAaA,KAAK,EAAC;AAAe;;EAC5DA,KAAK,EAAC;AAAoB;;EAAWA,KAAK,EAAC;AAAe;;;;;uBArDrEC,YAAA,CAyDYC,oBAAA;sBAxDV,MAuDM,CAvDNC,mBAAA,CAuDM,OAvDNC,UAuDM,G,4BAtDJD,mBAAA,CAA+D;MAA3DH,KAAK,EAAC;IAAuC,GAAC,UAAQ,qBAC1DG,mBAAA,CAgBM,OAhBNE,UAgBM,G,gBAfJF,mBAAA,CAKE;iEAJSG,KAAA,CAAAC,MAAM,GAAAC,MAAA;MACdC,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,UAAA,IAAAD,QAAA,CAAAC,UAAA,IAAAF,IAAA,CAAU;MAClBG,WAAW,EAAC,mBAAmB;MAC/Bd,KAAK,EAAC;mEAHGM,KAAA,CAAAC,MAAM,E,mBAKjBJ,mBAAA,CAQS;iEAPEG,KAAA,CAAAS,UAAU,GAAAP,MAAA;MAClBQ,QAAM,EAAAN,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,UAAA,IAAAD,QAAA,CAAAC,UAAA,IAAAF,IAAA,CAAU;MACnBX,KAAK,EAAC;kCAENG,mBAAA,CAA+B;MAAvBc,KAAK,EAAC;IAAE,GAAC,OAAK,oBACtBd,mBAAA,CAAoC;MAA5Bc,KAAK,EAAC;IAAO,GAAC,OAAK,oBAC3Bd,mBAAA,CAAqC;MAA7Bc,KAAK,EAAC;IAAM,GAAC,SAAO,mB,2DANnBX,KAAA,CAAAS,UAAU,E,MASXH,QAAA,CAAAM,aAAa,CAACC,MAAM,I,cAAhCC,mBAAA,CAEM,OAFNC,UAEM,EAFmE,8BAEzE,M,cACAD,mBAAA,CAwBQ,SAxBRE,UAwBQ,GAvBNnB,mBAAA,CAOQ,gBANNA,mBAAA,CAKK,MALLoB,UAKK,G,0BAJHpB,mBAAA,CAAyC;MAArCH,KAAK,EAAC;IAAqB,GAAC,MAAI,qB,0BACpCG,mBAAA,CAA0C;MAAtCH,KAAK,EAAC;IAAqB,GAAC,OAAK,qB,0BACrCG,mBAAA,CAAyC;MAArCH,KAAK,EAAC;IAAqB,GAAC,MAAI,qBACEY,QAAA,CAAAY,OAAO,I,cAA7CJ,mBAAA,CAAyD,MAAzDK,UAAyD,EAAV,OAAK,K,uCAGxDtB,mBAAA,CAcQ,iB,kBAbNiB,mBAAA,CAYKM,SAAA,QAAAC,WAAA,CAZcf,QAAA,CAAAM,aAAa,EAArBU,IAAI;2BAAfR,mBAAA,CAYK;QAZ8BS,GAAG,EAAED,IAAI,CAACE,EAAE;QAAE9B,KAAK,EAAC;