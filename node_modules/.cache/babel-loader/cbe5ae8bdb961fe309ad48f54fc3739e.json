\n      return tasks\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchTasks() {\r\n      const { data } = await api.get('/tasks')\r\n      this.tasks = data\r\n    },\r\n    formatDate(date) {\r\n      return new Date(date).toLocaleDateString('pt-BR')\r\n    },\r\n    async markAsCompleted(task) {\r\n      await api.put(`/tasks/${task.id}/complete`)\r\n      this.fetchTasks()\r\n    },\r\n    async deleteTask(task) {\r\n      await api.delete(`/tasks/${task.id}`)\r\n      this.fetchTasks()\r\n    },\r\n    exportPDF() {\r\n      alert('Funcionalidade de exportar PDF em breve!')\r\n    }\r\n  },\r\n  mounted() {\r\n    this.user = JSON.parse(localStorage.getItem('user'))\r\n    this.fetchTasks()\r\n  }\r\n}\r\n</script> "],"mappings":";;EAESA,KAAK,EAAC;AAAmB;;EACvBA,KAAK,EAAC;AAAwD;;EAgB3DA,KAAK,EAAC;AAAoB;;;EAEAA,KAAK,EAAC;;;;EAG1BA,KAAK,EAAC;;;EAYVA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAW;;EACjBA,KAAK,EAAC;AAAW;;;EACSA,KAAK,EAAC;;;;EACrBA,KAAK,EAAC;;;EAEjBA,KAAK,EAAC;AAAW;;;;EAoBtBA,KAAK,EAAC;AAAuB;;;uBA9DtCC,YAAA,CAuEYC,oBAAA;sBAtEV,MAqEM,CArENC,mBAAA,CAqEM,OArENC,UAqEM,GApEJD,mBAAA,CAiBM,OAjBNE,UAiBM,G,gBAhBJF,mBAAA,CAKE;iEAJSG,KAAA,CAAAC,MAAM,GAAAC,MAAA;MACdC,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,UAAA,IAAAD,QAAA,CAAAC,UAAA,IAAAF,IAAA,CAAU;MAClBG,WAAW,EAAC,kBAAkB;MAC9Bd,KAAK,EAAC;mEAHGM,KAAA,CAAAC,MAAM,E,mBAKjBJ,mBAAA,CAQS;iEAPEG,KAAA,CAAAS,MAAM,GAAAP,MAAA;MACdQ,QAAM,EAAAN,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,UAAA,IAAAD,QAAA,CAAAC,UAAA,IAAAF,IAAA,CAAU;MACnBX,KAAK,EAAC;kCAENG,mBAAA,CAA+B;MAAvBc,KAAK,EAAC;IAAE,GAAC,OAAK,oBACtBd,mBAAA,CAA0C;MAAlCc,KAAK,EAAC;IAAS,GAAC,WAAS,oBACjCd,mBAAA,CAA6C;MAArCc,KAAK,EAAC;IAAW,GAAC,YAAU,mB,2DAN3BX,KAAA,CAAAS,MAAM,E,GAQjBZ,mBAAA,CAAwF,QAAxFe,UAAwF,G,2CAAvD,SAAO,IAAAf,mBAAA,CAAiC,WAAAgB,gBAAA,CAA3BP,QAAA,CAAAQ,aAAa,CAACC,MAAM,kB,2CAAO,UAAQ,G,MAEvET,QAAA,CAAAQ,aAAa,CAACC,MAAM,I,cAAhCC,mBAAA,CAEM,OAFNC,UAEM,EAFmE,8BAEzE,M,cACAD,mBAAA,CAsCQ,SAtCRE,UAsCQ,G,4BArCNrB,mBAAA,CAQQ,gBAPNA,mBAAA,